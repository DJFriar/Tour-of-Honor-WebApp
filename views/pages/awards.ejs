<head>
  <%- include('../partials/head'); %>
</head>

<body>
  <div class="pageWrapper">
    <% if(activeUser) { %>
      <%- include('../partials/topnavUser'); %>
    <% } else { %>
      <%- include('../partials/topnav'); %>
    <% } %>
    <% if(NotificationText) { %>
      <%- include('../partials/notice'); %>
      <% } %>
      <main>
      <h1>IBA Awards</h1>
      <p>Below you will find a list of riders who have earned IBA Rides.</p>
      <div>&nbsp;</div>
      <table class="uk-table uk-table-striped uk-table-small" id="awardsTable">
        <thead>
          <tr>
            <th>ID</th>
            <th>Rider</th>
            <th>Flag Number</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Award Earned</th>
            <th>Date Earned</th>
          </tr>
        </thead>
      </table>
    </main>
    <footer>
      <%- include('../partials/footer'); %>
    </footer>
  </div>
  <script type="text/javascript" src="js/awards.js"></script>
</body>
