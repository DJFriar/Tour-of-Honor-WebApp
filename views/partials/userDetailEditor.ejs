<div class="modal-content">
  <span class="close">&times;</span>
  <h3 id="editUserHeading" class="toh-mb-18">Edit User</h3>
  <form class="uk-grid-small uk-form-stacked" uk-grid>
    <input class="uk-input" id="EditUserID" type="hidden" placeholder="">
    <div class="uk-width-1-5@s">
      <label class="uk-form-label" for="EditFlagNumber">Flag Number</label>
      <input disabled class="uk-input toh-input" id="EditFlagNumber" type="text">
    </div>
    <div class="uk-width-2-5@s">
      <label class="uk-form-label" for="EditFirstName">First Name</label>
      <input class="uk-input toh-input" id="EditFirstName" type="text" placeholder="">
    </div>
    <div class="uk-width-2-5@s">
      <label class="uk-form-label" for="EditLastName">Last Name</label>
      <input class="uk-input toh-input" id="EditLastName" type="text" placeholder="">
    </div>
    <div class="uk-width-3-5@s toh-mt-12">
      <label class="uk-form-label" for="EditAddress">Address</label>
      <input class="uk-input toh-input" id="EditAddress" type="text" placeholder="">
    </div>
    <div class="uk-width-2-5@s toh-mt-12">
      <label class="uk-form-label" for="EditCellNumber">Cell Number</label>
      <input class="uk-input toh-input" id="EditCellNumber" type="phone" placeholder="">
    </div>
    <div class="uk-width-1-3@s">
      <label class="uk-form-label" for="EditCity">City</label>
      <input class="uk-input toh-input" id="EditCity" type="text" placeholder="">
    </div>
    <div class="uk-width-1-3@s">
      <label class="uk-form-label" for="EditState">State</label>
      <input class="uk-input toh-input" id="EditState" type="text" placeholder="">
    </div>
    <div class="uk-width-1-3@s">
      <label class="uk-form-label" for="EditZipCode">Zip Code</label>
      <input class="uk-input toh-input" id="EditZipCode" type="text" placeholder="">
    </div>
    <div class="uk-width-4-5@s toh-mt-12">
      <label class="uk-form-label" for="EditEmail">Email</label>
      <input class="uk-input toh-input" id="EditEmail" type="text" placeholder="">
    </div>
    <div class="uk-width-1-5@s toh-mt-12">
      <label class="uk-form-label" for="isAdmin">&nbsp;</label>
      <input class="uk-checkbox" id="isAdmin" type="checkbox" placeholder="">&nbsp; Admin
    </div>

    <div class="uk-width-1-1@s">
      <div class="uk-align-right">
        <label class="uk-form-label">&nbsp;</label>
        <button class="uk-button uk-button-secondary button-rounded cancelButton" id="cancelChangesButton" type="button">Cancel</button>
        <button class="uk-button uk-button-primary button-rounded toh-ml-8" id="saveUserChangesButton" type="button">Save</button>
      </div>
    </div>

  </form>
</div>